        <text key="wikipedia" text="Wikipedia" default="" delete_if_empty="true" />
        <space />
        <link text="Key:opening_hours @ osm-Wiki" href="https://wiki.openstreetmap.org/wiki/Key:opening_hours" />	
        <link text="Tag:seamark:type=bridge @ osm-Wiki" href="https://wiki.openstreetmap.org/wiki/Tag:seamark:type%3Dbridge" />
      </item>
          <item name="lifting bridge" icon="https://raw.githubusercontent.com/OpenNauticalChart/josm/master/seamark_type_bridge-preset/liftingbridge.png" type="node">
        <label text="use decimal point:" />
